@import '/assets/main.acss';

.item { 
    width: 100%; 
    height: 150rpx;
    line-height: 150rpx; 
    border:1rpx solid #eeeeee;
    background-color: white; 
    display: flex;
    justify-content: space-around;
    align-items: center;
    padding: 60rpx 50rpx;
    font-size: 28rpx;
    background-color: #ecf7fd;
}

.phoneNum {font-size: 34rpx;}

.school {font-size: 34rpx;}

.item>view{ flex: 1;}

.item>view:last-child{text-align: right;margin-right: 10rpx;}