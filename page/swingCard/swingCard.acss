@import "/assets/public.acss";

page {
  background:#fff;
}
.header {
  margin: 40rpx;
  height: 280rpx;
  border-radius: 16rpx;
  background-image: url('data:image/png;base64,')
}
.header > view {
  font-size: 16px;
  padding: 36rpx 0 10rpx 46rpx;  
}
.mid {
  margin: 40rpx;
}
.footer{
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin: 0 15rpx;
}
.footer > button {
  width: 45%;;
  border-radius: 44rpx;  
}
.footer > button:first-child {
  border: 1rpx solid #4aa7f4;
}
.scanning-box {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
}
.shadow {
  width: 100%;
  height: 100%;
  background-color:#999;
  opacity: 0.95;
}
.detail {
  position: absolute;
  background: #fff;
  top: 5%;
  margin: 10%;
  width: 80%;
  height: 75%;
}
.detail > view:first-child {
  padding: 40rpx 0;
  text-align: center;
  color: #000;
}
.detail > view:nth-child(2){
  text-align: center;
}
.detail > view:last-child {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 40rpx 0;
}
.scan-btn {
  width: 70%;
  border-radius: 22px;
}
.image {
  height: 600rpx;
}
.creditCard {
  padding: 40rpx 0; 
}
.countDown {
  border-radius: 50%;
  background-color: #4AA7F4;
  width: 108rpx;
  height: 108rpx;
  margin: 0 auto;
}
.bindSucc {
  text-align: center;
}
.bindSucc > image {
  width: 140rpx;
  height: 140rpx;
}