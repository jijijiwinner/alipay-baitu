@import '/assets/public.acss';

page {
  background: #fff;
}
.header {
  padding: 50rpx 0;
  height: 360rpx;  
  background-size: 100%;
  background-repeat: no-repeat;
  background-image: url("data:image/png;base64,");  
}
.header > view {
  text-align: center;
}
.header > view:first-child {
  margin: 40rpx 0 40rpx 0;
}
.mid {
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-bottom: 1rpx solid #e6e7e8;
}
.mid > view {
  width: 50%;
  text-align: center;
}
.mid > view:first-child {
  border-right: 1rpx solid #e6e7e8;
  margin: 30rpx;  
}
.mid > view > image {
  width: 60rpx;
  height: 60rpx;
}
.footer {
  display: flex;
  justify-content: center;
  align-items: center;
}
.footer > view {
  width: 55%;
  margin: 42rpx 15rpx;  
}
.footer > view > button {
  border-radius: 44rpx;
}
.footer > view > button:first-child {
  border: 1rpx solid #4aa7f4;
}