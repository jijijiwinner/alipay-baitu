@import '/assets/public.acss';

.machine-header {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 360rpx;
  background-size: 100% 100%;
  background-repeat:  no-repeat;
  background-image: url("data:image/png;base64,");
}
.machine-header >view {
  padding: 0 30rpx;
  color: #fff;
}
.machine-header >view:first-child > image {
  width: 1.3rem;
}
.machine-mid {
  text-align: center;
}
.machine-mid > view:first-child {
  color: #59A4E3;  
  font-size: 16px;
  margin:40rpx 0 30rpx 0;
}
.machine-mid > .model {
  display:  flex;
  justify-content: center;
  align-content: center;
  align-items: center;
  flex-wrap: wrap;
}
.machine-mid > .model > view {
  border-radius:50%;
  width: 200rpx;
  height: 200rpx;
  line-height: 2rem;
  margin: 10rpx 50rpx 40rpx 50rpx;
} 
.selected{
  color: #ffffff;
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}
.normal{
  color: #50a6ef;
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}
.redEnve {
  padding:20rpx;  
  position: fixed;
  bottom: 20%;
  right: 0%;
}
.redEnve >image {
  width: 90rpx;
  height: 110rpx;
}
.machine-pay {
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-top: 1rpx solid #bdc1c3;
  margin: 0 50rpx 0 50rpx;
  padding: 25rpx 0;
}
.machine-pay >image {
  width: 35rpx;
  height: 35rpx;
}
.btn {
  display: flex;
  justify-content: center;
  align-items: center;
  padding:30rpx 0;
}
.bg_color1 {
  background: #50A6EF;
  color: #fff;
  width: 85%;
  border-radius: 6px;
}
.popUp {
  z-index: 1000;
}
.upMain{
  background: white;
  z-index: 1000;
}
.payHeader {
  display:flex; 
  align-items:center;
  padding: 40rpx 60rpx 20rpx 60rpx;
}
 .payHeader > .header-top >.payImg {
  width: 30rpx;
  height: 30rpx;
}
.typeTwo {
  margin-left:20rpx;
}
.typeTwo >text {
  color:red;
  border:1rpx solid red;
  padding: 10rpx;  
  font-size: 24rpx;
}
.typeThree {
  display: flex;
  align-items: center;
  margin-left:20rpx;
}
.typeThree > text:first-child{
  color:orange;
  border:1rpx solid orange;
  font-size: 24rpx;
  padding: 10rpx;
}
.freePay {
  display: flex;
  align-items: center;
}
.freePay >view:first-child {
  padding-left: 160rpx;
  font-size: 24rpx;  
  color: #50A6EF;  
}
.threeImg {
  width: 30rpx;
  height: 30rpx;
  color: #50A6EF;
}
.machine-footer {
  position: fixed;
  bottom: 0;
  width: 100%;
  margin:  0 auto;
}
