/* 最外层盒子 */
.school {
  width: 100%;
  padding: 5rpx 0;
}
/* 搜索盒子 */
.search {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

/* 搜索输入框 */
.search input {
  margin: 30rpx 0;
  font-size: 28rpx;  
  padding-left: 30rpx;
  background: #E8E8E8;
}
.search button{
  padding: 0;
  margin: 0;
  position: absolute;
  top: 0.18rem;
  right: 145rpx;
  background: none;  
}
.search button icon {
  width: 50rpx;
  height: 0.58rem;
  background: none;  
}
.search button::after {
  content: '';
  display: none;
  outline: none;
  border: none;
}
.a-button {
  border: none;
}
/* 输入的样式 */
.searchColor {
  color: #929399;
}
/* 学校列表样式 */
.content {
  margin: 20rpx 0;
  padding: 15rpx;
  font-size: 28rpx;
  border-bottom: solid 1px #eee;  
}
/* 扫码进入提示框 */
.pop{
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
}
.pop .shadow{
  width: 100%;
  height: 100%;
  background-color: gray;
  opacity: 0.9;
}
.pop .contents{
  position: absolute;
  top: 350rpx;
  width: 72%;
  margin-left: 14%;
  height: 56vw;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}
.contents view{
  width: 100%;
  text-align: center;
  margin-top: 30rpx;
}
.pop_header text{
  color: rgb(192, 223, 248);
}
.pop_center{
  min-height: 120rpx;
}
.pop_center text{
  font-size: 40rpx;
  color: #FFFFFF;
}
.pop_tips text{
  color: #469BE2;
}
.pop_bottom button{
  display: inline-block;
  width: 40%;
  height: 60rpx;
  line-height: 60rpx;
  margin:0 10rpx;
  border-radius: 40rpx;
  font-size: 30rpx;
}
.pop_bottom button:first-child{
  background-color: #FFFFFF;
  color: #469BE2;
  border: solid 1rpx #469BE2;
}
.pop_bottom button:last-child{
  background-color: #469BE2;
  color: #FFFFFF;
}