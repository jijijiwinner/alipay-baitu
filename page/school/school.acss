
/* 最外层盒子 */
.school {
  width: 100%;
  padding: 15rpx 0;
}
/* 搜索盒子 */
.search {
  padding: 10rpx;
}

/* 搜索输入框 */
.search .schSearch {
  display: flex;
  justify-content: center;
  align-items: center;
  align-content: center;
  box-sizing: border-box;
  padding: 10rpx 0rpx;
  margin: 30rpx 80rpx;
  background: #E8E8E8;
}
/* 输入框 */
.schSearch input {
  font-size: 28rpx;  
}
/* 搜索框的图片 */
.schSearch image {
  width: 60rpx;
  height: 60rpx;
  box-sizing: border-box; 
  margin-left: -180rpx;
}
/* 输入的样式 */
.searchColor {
  display: flex;
  align-items: center;
  color: #929399;
}
/* 学校列表样式 */
.content {
  margin: 20rpx 0;
  padding: 0 20rpx;
  font-size: 28rpx;
  border-bottom: solid 1px #eee;  
}
.a-input{
  background: none;
  padding: 0;
}
/* 扫码进入提示框 */
.pop{
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
}
.pop .shadow{
  width: 100%;
  height: 100%;
  background-color: gray;
  opacity: 0.9;
}
.pop .contents{
  position: absolute;
  top: 200rpx;
  width: 72%;
  margin-left: 14%;
  height: 56vw;
background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}
.pop_school {
  text-align: center;
  margin-top: 50rpx;
}
.pop_school .opacity{
  opacity: 0.5;
  margin-bottom: 40rpx;
}
.pop_school view {
  color: white;
  font-size: 32rpx;
}
.pop_choose{
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-top: 150rpx;
}
.pop_choose .no_school{
  color: #4592BE;
  border: 1rpx solid #4592BE;
  border-radius: 20rpx;
  padding: 20rpx;
}
.pop_choose .pop_ok{
  width: 200rpx;
  text-align: center;
  color: white;
  background:#479BE3; 
  border-radius: 20rpx;
  padding: 20rpx;
}