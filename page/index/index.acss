@import '/assets/main.acss';
.image{
  width: 100%;
}
.bar{
  width: 70%;
  margin: 10rpx 15% 0 15%;
  display: flex;
  text-align: center;
}
.bar>view{
  flex: 1;
  padding: 20rpx;
  border:1rpx solid #4ab6f4;
}
.bar>view:first-child{
  border-radius: 20rpx 0 0 20rpx;
}
.bar>view:last-child{
  border-right: solid 1rpx #4ab6f4;
  border-radius:0 20rpx 20rpx 0;
}
canvas{
   width: 100%;  
   height: 100%;
}
.child{
  position: relative; 
  top: 50%; 
  left: 50%; 
  transform: translate(-50%, -50%);
  text-align: center;
  border: solid 1px #c4e8f8;
  background-color: #c4e8f8;
  background-image: url('data:image/png;base64,'); 
  background-repeat: no-repeat;
  /* border:solid 1px red; */
}
.close{
  position: relative; 
  top: -51%; 
  left: 50%; 
  transform: translate(-50%, -50%); 
  text-align: center;
  border: solid 1px #eeeeee;
  background-color: #c4e8f8;
  font-size: 18px;
  z-index: 100;
}
.select{
  background-color: #78caf2;
  color: white;
}
.shadow{
  width: 100%;
  height: 100%;
  background-color: white;
  z-index: 10;
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0.95;
}
.detail{
  z-index: 30;
  position: absolute;
  top: 20%;
  left: 10%;
  width: 80%;
}
.detail>view{
  border-bottom:solid 1rpx #eeeeee;
  height:100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 40rpx;
  color: #2b7d9e;
  background-color:#D9EFFA;
  margin-top: 20rpx;
  border-radius: 20rpx;
}
.detail text{
  width:100%;
  display:block;
  text-align:center;
  font-size:36rpx
}
span{
  display:inline-block;
  margin-left: 10rpx; 
}