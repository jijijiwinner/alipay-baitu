@import "/assets/public.acss";
.header {
  width: 100%;
  height: 100rpx;
  background-color: #4AA7F4;
}
.advert {
  position: absolute;
  top: 50rpx;
  margin-left: 2%;
  width: 96%;
  text-align: center;
  display: flex;
  justify-content: center;
}
.advert > swiper {
  height: 328rpx;
  border-radius: 10rpx;
}
swiper image {
  width: 100%;
  height: 328rpx;
  border-radius: 10rpx;
}
.sweep-area {
  width: 500rpx;
  height: 480rpx;
  margin: 400rpx auto;
  margin-bottom: 0;
}
.notice {
  position: absolute;
  top: 100%;
  left: 0%;
}
.scan {
  width: 380rpx;
  height: 380rpx;
  margin:  0 auto;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url("data:image/png;base64,");
}
.sweep-area > text {
  display: block;
  width: 380rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  margin-left: 60rpx;
  background-color: #E5EEF5;
}
.sweep-area > image {
  width: 380rpx;
  height: 380rpx;
  margin: 0 auto;
  margin-left: 60rpx;
}
.sweep-btn {
  width: 400rpx;
  height: 80rpx;
  line-height: 80rpx;  
  margin: 0 auto;
  background-color: #3b85c4;
  border-radius: 42rpx;
  position: relative;
  z-index: 100;
}
.sweep-btn > view:first-child {
  display: flex;
  text-align: center;
}
.sweep-btn > view:first-child >view {
  height: 100%;
  flex: 1;
  color:#97CFFF;
}
.sweep-btn > view:last-child {
  width: 200rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  margin: 0 auto;
  position: absolute;
  top: 0;
  color: #4C80AB;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url("data:image/png;base64,");
}
/* 切换扫一扫 */
@keyframes scaning {
  0% {left:200rpx}
  100% {left: 0rpx;}
}
/* 切换二维码 */
@keyframes ewm {
  0% {left: 0}
  100% {left: 200rpx;}
}
.stopHot {
  width: 340rpx;
  height: 340rpx;
  margin: 0 auto;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url("data:image/png;base64,");
}
.bg_bottom {
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
  width: 100%;
  height: 150rpx;
  background-repeat: no-repeat;
  position: fixed;
  bottom: 0;
  z-index: 1;
}
.shadow{
  width: 100%;
  height: 100%;
  background-color: white;
  z-index: 10;
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0.95;
}
.detail{
  z-index: 30;
  position: absolute;
  top: 20%;
  left: 10%;
  width: 80%;
}
.detail>view{
  border-bottom:solid 1rpx #eeeeee;
  height:100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 40rpx;
  color: #2b7d9e;
  background-color:#D9EFFA;
  margin-top: 20rpx;
  border-radius: 20rpx;
}
.detail text{
  width:100%;
  display:block;
  text-align:center;
  font-size:36rpx
}
span{
  display:inline-block;
  margin-left: 10rpx; 
}
/* 主页活动弹窗 */
.popUp {
  position: fixed;
  width:100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 100;
}
.act-shadow {
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: 0.7;
}
.act-detail{
  position: absolute;
  top: 8%;
  width: 90%;
  height: 90%;
  margin: 5%;
}
.act-detail > image:first-child {
  position: absolute;
  top: -9%;
  right: 3%;
  width: 75rpx;
  height: 75rpx;
}
.act-detail > image:nth-child(2){
  width: 100%;
}
.act-detail > view {
  width: 100%;
  text-align: center;
  padding-top: 56rpx;
}
.act-detail > view > image {
 width: 50%;
}