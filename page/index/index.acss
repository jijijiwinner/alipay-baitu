@import "/assets/public.acss";
.header {
  width: 100%;
  height: 100rpx;
  background-color: #4AA7F4;
}
.advert {
  position: absolute;
  top: 50rpx;
  margin-left: 2%;
  width: 96%;
  text-align: center;
  display: flex;
  justify-content: center;
}
.advert > swiper {
  height: 328rpx;
  border-radius: 10rpx;
}
swiper image {
  width: 100%;
  height: 328rpx;
  border-radius: 10rpx;
}
.sweep-area {
  width: 500rpx;
  height: 480rpx;
  margin: 400rpx auto;
  margin-bottom: 0;
}
.notice {
  position: absolute;
  top: 100%;
  left: 0%;
}
.scan {
  width: 380rpx;
  height: 380rpx;
  margin:  0 auto;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url("data:image/png;base64,");
}
.sweep-area > text {
  display: block;
  width: 380rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  margin-left: 60rpx;
  background-color: #E5EEF5;
}
.sweep-area > image {
  width: 380rpx;
  height: 380rpx;
  margin: 0 auto;
  margin-left: 60rpx;
}
.sweep-btn {
  width: 400rpx;
  height: 80rpx;
  line-height: 80rpx;  
  margin: 0 auto;
  background-color: #3b85c4;
  border-radius: 42rpx;
  position: relative;
}
.sweep-btn > view:first-child {
  display: flex;
  text-align: center;
}
.sweep-btn > view:first-child >view {
  height: 100%;
  flex: 1;
  color:#97CFFF;
}
.sweep-btn > view:last-child {
  width: 200rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  margin: 0 auto;
  position: absolute;
  top: 0;
  color: #4C80AB;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url("data:image/png;base64,");
}
/* 切换扫一扫 */
@keyframes scaning {
  0% {left:200rpx}
  100% {left: 0rpx;}
}
/* 切换二维码 */
@keyframes ewm {
  0% {left: 0}
  100% {left: 200rpx;}
}