@import '/assets/public.acss';

.header {
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding: 60rpx;
  background-size: 100%;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,')
}
.header-left >image {
  width: 114rpx;
  height: 114rpx;
  background: #fff;
  border-radius: 50%;
  margin-right: 20rpx;
}
.header-mid {
  margin: 0 40rpx;
}
.header-mid > view{
  padding:4rpx 0;
}
.header-right {
  width: 1.3rem;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1rpx solid #fff;
  border-radius: 36rpx;
  padding: 8rpx 0;
}
.mid {
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 80rpx;
  margin-bottom: 20rpx;
}
.mid-left > view:first-child > image {
  width: 24rpx;
  height: 24rpx;
  line-height: 24rpx;
  margin-top: 5rpx;
  vertical-align: middle;
}
.mid-left > view:last-child {
  margin-left: 20rpx;
}
.mid-right {
  border: 1rpx solid #FF7900;
  border-radius: 36rpx;
  padding: 10rpx 40rpx;
}
.am-list-content {
  font-size: 13px;
  color: #292929;
}
.am-list-content > image {
  width: 44rpx;
  vertical-align: middle;
  margin-right: 20rpx;
  margin-top: -5px;
}
.image {
  width: 25rpx;
  color: #fff;
  vertical-align: middle;
  margin-right: 5px;
}