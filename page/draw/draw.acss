page {
  background: #fff;
}
.header{
  width: 100%;
  height: 400rpx;
  padding:0 50rpx;
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,')
}
.header>view{
  text-align: center;
}
.header image{
  width: 150rpx;
  height: 150rpx;
  margin: 100rpx 0 30rpx 0;
}
.header text{
  display: block;
  color: #ffffff;
}
.draw-button{
  width: 100%;
  overflow: hidden;
}
.cover{
  width: 400rpx;
  text-align: center;
  margin: 0 auto;
}
.animation{
  width:1440rpx;
  height: 880rpx; 
  margin-left: -520rpx;
  background-color: transparent;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  position: relative;
  z-index: 100;
  overflow: hidden;
}
.animation{
  background-image: url('data:image/png;base64,')
}
.draw-flow{
  position: relative;
}
.draw-flow>view{
  position: absolute;
  top: -400rpx;
  width: 100%;
  height: 400rpx;
  background-repeat: repeat-x;
  background-size: 50% 100%;
  z-index: 50;
  overflow-x: hidden;
}
.draw-flow>view:first-child{
  right: 0;
  background-image: url('data:image/png;base64,');
}
.draw-flow>view:last-child{
  background-image: url('data:image/png;base64,');
}
@keyframes drawFlow1{
  0% {background-position: 0 0; top: -400rpx;}
  100% {background-position: 3000rpx; top: -800rpx;}
}
@keyframes drawFlow2{
  0% {background-position: 0 0; top: -400rpx;}
  100% {background-position: -3000rpx 0; top: -800rpx;}
}
.stop{
  width: 380rpx;
  height: 380rpx;
  line-height: 380rpx;
  margin: 95rpx auto;
  border-radius: 190rpx;
}
.stop>text{
  font-size: 40rpx;
}
.draw-footer{
  width: 100%;
  text-align: center;
  position: absolute;
  bottom: 200rpx;
  z-index: 120;
}
.draw-footer text{
  color: #50a6ef;
}
